{
    "name": "kyvour/temosh",
    "description": "Test MongoDB Shell",
    "type": "library",
    "license": "MIT",
    "authors": [
        { "name": "Andrew Yustyk", "email": "a.yustyk@gmail.com" }
    ],
    "bin": [
        "temosh"
    ],
    "autoload": {
        "psr-4": { "Temosh\\": "src/" }
