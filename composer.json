        "ext-mongodb": "*",
        "symfony/console": "^3.3",
        "mongodb/mongodb": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7.20",
        "symfony/phpunit-bridge": "^3.3",
        "hoa/console": "^3.17"
    }
}
